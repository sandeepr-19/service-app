doctype html(lang='en')
head
    meta(charset='UTF-8')
    |     
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    |     
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    // CSS only
    link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css', rel='stylesheet', integrity='sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx', crossorigin='anonymous')
    // JavaScript Bundle with Popper
    script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js', integrity='sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa', crossorigin='anonymous')
    |     
    link(rel='stylesheet', href='index.css')
    |
    link(rel="stylesheet", href="sidebar.css")
    |     
    title service_app   

body.w-100
  include ../components/sidebar.pug
  // header
  .d-flex.py-auto.float-end.h-100#page
    .mx-auto.my-auto
        table#table.table.table-hover.w-75.mx-auto.overflow-auto(style='max-height: 200px')
            thead
                tr
                    th Attendee
                    th Complaint
                    th Product details
                    th accepted date 
                    th finished date
            tbody#ser_table
                each objt in obj 
                    tr 
                        td #{objt['attendee-name']}
                        td #{objt['complaint']}
                        td #{objt['product-details']}
                        td #{objt['accepted-date']}
                        td #{objt['finished-date']}
